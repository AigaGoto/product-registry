<p id="notice"><%= notice %></p>

<h1>Peoducts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Vendor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @peoducts.each do |peoduct| %>
      <tr>
        <td><%= peoduct.name %></td>
        <td><%= peoduct.price %></td>
        <td><%= peoduct.vendor %></td>
        <td><%= link_to 'Show', peoduct %></td>
        <td><%= link_to 'Edit', edit_peoduct_path(peoduct) %></td>
        <td><%= link_to 'Destroy', peoduct, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Peoduct', new_peoduct_path %>
